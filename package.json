    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "express-rate-limit": "7.2.0",
    "express-session": "1.18.0",
    "helmet": "7.1.0",
    "jsonwebtoken": "9.0.2",
    "mercadopago": "2.0.9",
    "moment": "2.30.1",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "nodemailer": "6.9.9",
    "path": "0.12.7",
    "pg": "8.11.3",
    "pg-hstore": "2.3.4",
    "sanitize-filename": "1.6.3",
    "sequelize": "6.35.2",
    "supertest": "6.3.4",
    "tiny-csrf": "1.1.4",
    "uuid": "9.0.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz"
  },
  "description": "",
  "devDependencies": {
    "chai": "4.4.1",
    "eslint": "8.56.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.1.1",
    "husky": "9.0.10",
    "mocha": "10.2.0",
    "nodemon": "3.0.3"
  },
  "engines": {
    "node": "18"
  },
  "eslintConfig": {
    "extends": "standard"
  },
  "http.proxySupport": "off",
  "imports": {
    "#CONSTANTS": "./src/constants.js",
    "#SERVICES/*": "./src/services/*.js",
    "#DB_CONNECTION": "./src/db.js",
    "#ERRORS": "./src/errors/index.js",
    "#CONTROLLERS": "./src/controllers/index.js",
    "#HANDLERS": "./src/handlers/index.js",
    "#TESTS/*": "./src/tests/*/index.js",
    "#UTILS/*": "./src/utils/*.js",
    "#ROUTES": "./src/routes/index.js",
    "#MIDDLEWARES": "./src/middlewares/index.js",
    "#SERVER": "./src/app.js"
  },
  "keywords": [
    "REST API",
    "BACKEND",
    "EXPRESS",
    "NODE",
    "POSTGRESQL",
    "SEQUELIZE",
    "JAVASCRIPT"
  ],
  "license": "ISC",
  "main": "index.js",
  "name": "ide-pinturerias-rest-api",
  "scripts": {
    "lint": "eslint --fix . --max-warnings 0",
    "resetCarts": "node src/scripts/resetCarts.js",
    "resetCategories": "node src/scripts/resetCategories.js",
    "resetOrders": "node src/scripts/resetOrders.js",
    "resetProducts": "node src/scripts/resetProducts.js",
    "resetProductsCategories": "node src/scripts/resetProductsCategories.js",
    "resetUsers": "node src/scripts/resetUsers.js",
    "secureImages": "node src/scripts/secureImages.js",
    "setProviderToProducts": "node src/scripts/setProviderToProducts.js",
    "deps-check": "pnpx npm-check",
    "dev": "nodemon ./src/index.js -L --trace-warnings",
    "start": "node src/index.js",
    "test": "mocha src/tests/index.spec.js",
    "test-env": "mocha src/tests/env.spec.js",
    "test-server": "mocha src/tests/server.spec.js",
    "run-actions": "gh act -P ubuntu-20.04=nektos/act-environments-ubuntu:20.04",
    "install-snyk": "pnpm add -g snyk",
    "snyk-test": "snyk test",
    "check-updates": "pnpx npm-check-updates -ui"
  },
  "version": "3.0.0"
}
